#include<stdio.h>
int main()
{
	int n;
	printf("Enter n value");
	scanf("%d",&n);
	int x,y;
  printf("enter x value");
	scanf("%d",&x);
  printf("enter y value");
  scanf("%d",&y);
	int temp,sum=0,count=1;
	while(n>0)
	{
		r=n%10;
		if(temp==x)
		{
			temp=y;
		}
		sum=sum+temp*count;
		count=count*10;
		n=n/10;
	}
	printf("%d",sum);
}
